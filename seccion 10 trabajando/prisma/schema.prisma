generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define your models here.
// Example:
// model Place {
//   id        Int      @id @default(autoincrement())
//   name      String
//   createdAt DateTime @default(now())
// }

model Marker {
  id        Int    @id @default(autoincrement())
  id_public String @unique
  title     String
  categoria String
  geojson   Json
}
